{
  "manifest_version": 2,
  "name": "GitHub MultiAccount",
  "description": "Chrome extension to switch between multiple accounts on Github",
  "version": "3.3",
  "permissions": [
    "cookies",
    "tabs",
    "https://github.com/",
    "declarativeContent"
  ],
  "icons": {
    "128": "icons/logo.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": "icons/logo.png"
  },
  "author": "Alexandra Kreidich"
}